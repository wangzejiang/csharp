            function closeWxBindWarp() {
                __close('idw_subscribewin');
                $('#mcover').hide();
                var note = $("#noShowWxBind:checked");
                window.wx_stime && window.clearInterval(window.wx_stime);
                if (note && note.length > 0) {
                    $.get("/ajax/NoShowWxBind.ashx?action=ent");
                }
            }
            function show_my_wxbind() {
                $("#mcover").show();
                $("#idw_subscribewin").show();
                window.GetWxLoginUrl({ img: $('#jw_wxbind_pic'), positiontype: 6 });
                window.wxlogin_success = function () {
                    window.location.href = window.location.href;
                }
            }
            $(function () {
                $("#wxleadpic").hover(function () {
                    $("#wxleadpic_warp").show();
                }, function () {
                    $("#wxleadpic_warp").hide();
                });
            });
        </script>
    
