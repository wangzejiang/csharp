        <script type="text/javascript">
            function closeUpdateSalaryJobWarp() {
                __close('idw_subscribewinSalary');
                $('#mcoverSalary').hide();
                window.wx_stime && window.clearInterval(window.wx_stime);
            }
            function show_my_updateSalary() {
                $("#mcoverSalary").show();
                $("#idw_subscribewinSalary").show();
            }
            function checkNeedUpdateSalary() {
                // 如果存在薪资为面议的职位，则在点击刷新职位时弹出弹窗提示
                var bl = 0;
                if (bl == 1) {
                    show_my_updateSalary();
                    return;
                }
               RefreshOffer();
            }
            function checkNeedUpdateSalary1() {
                // 如果存在薪资为面议的职位，则在点击刷新职位时弹出弹窗提示
                var bl = 0;
                 if (bl == 1) {
                     show_my_updateSalary();
                     return;
                 }
                 __open1("refreshoffer", { caption: "刷新招聘信息", w: 400, h: 450, scrolling: "no", url: "/ent/refresh_offer1.aspx" });
             }
            // 用于快速选择月薪
            function SelectMinMaxSalary(obj) {

                var ele = $(obj).prevAll();
                var salaryLow = $(ele[1]);
                var salaryHigh = $(ele[0]);
                var value = $(obj).val();
                if (value === "") {
                    salaryLow.val("");
                    salaryHigh.val("");
                } else if (value === "1千以下") {
                    salaryLow.val("100");
                    salaryHigh.val("1000");
                } else if (value === "5万以上") {
                    salaryLow.val("50000");
                    salaryHigh.val("100000");
                } else {
                    var salaryValue = value.split("-");
                    if (salaryValue[1].indexOf("万") != -1) {
                        salaryLow.val(salaryValue[0] * 10000);
                        salaryHigh.val(salaryValue[1].substring(0, salaryValue[1].indexOf("万")) * 10000);
                    } else {
                        salaryLow.val(salaryValue[0] * 1000);
                        salaryHigh.val(salaryValue[1].substring(0, salaryValue[1].indexOf("千")) * 1000);
                    }
                }
                CheckMinMaxSalary(obj, true);

            }

            function CheckSalary() {

                $(".tbsalary").blur();
                var haveWarn = false;
                $(".checkSalary").each(function () {
                    if ($(this).html() != "")
                        haveWarn = true;
                });
                return !haveWarn;

            }

            function CheckMinMaxSalary(obj, showTip) {

                var td = $(obj).parent();

                var ele = $(td).children();
                var lowValue = $(ele[0]).val();
                var highValue = $(ele[1]).val();
                var salaryTip = $($(ele[3]).children("span"));

                var errStr = "";
                if (lowValue === "") {
                    errStr = "请输入最低薪资";
                } else if (highValue === "") {
                    errStr = "请输入最高薪资";
                } else if (isNaN(lowValue)) {
                    errStr = "最低薪资为大于等于0的整数";
                } else if (isNaN(highValue) || highValue == 0) {
                    errStr = "最高薪资为大于0的整数";
                } else if (lowValue < 500 && highValue > 1000) {
                    errStr = "当前薪资最高为1000元";
                } else if (lowValue >= 500 && lowValue * 2 < highValue) {
                    errStr = "最高薪资最多为最低薪资的2倍";
                } else if (parseInt(lowValue) > parseInt(highValue)) {
                    errStr = "最高薪资不能小于最低薪资";
                } else if (parseInt(highValue) > 100000) {
                    errStr = "最高薪资不能大于10万元";
                }

                if (errStr.length > 0 && showTip) {
                    salaryTip.html(errStr);
                    $(ele[3]).show();
                    return false;
                }

                salaryTip.html("");
                $(ele[3]).hide();
                return true;

            }

            // 月薪输入检查，只能输入数字
            function CheckInputSalary(obj) {
                var value = $(obj).val();
                value = value.replace(/(^0+)|([^0123456789])/g, ""),
                    $(obj).val(value);

            }

        </script>
    
</div>

    

    <script type="text/javascript">

            function stopBubble(e) {
                //如果提供了事件对象，则这是一个非IE浏览器
                if (e && e.stopPropagation)
                    //因此它支持W3C的stopPropagation()方法
                    e.stopPropagation();
                else
                    //否则，我们需要使用IE的方式来取消事件冒泡
                    if(window.event){window.event.cancelBubble = true;}
            }
    </script>

